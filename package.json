    "stylelint-prettier": "^4.0.2",
    "stylelint-scss": "^5.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-vue-components": "^0.26.0",
    "util": "^0.12.5",
    "vite": "^5.0.11",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-progress": "^0.0.7",
    "vite-plugin-restart": "^0.4.0",
