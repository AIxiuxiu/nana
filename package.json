{
  "name": "nana",
  "version": "0.0.1",
  "main": "app/dist/main/index.js",
  "author": "安娜网",
  "description": "安娜",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "vite --mode=development --host",
    "dev": "concurrently -n=R,P,M -c=green,yellow,blue \"npm run dev:render\" \"npm run dev:preload\" \"npm run dev:main\"",
    "dev:render": "vite --mode=development",
    "dev:preload": "cross-env NODE_ENV=development node script/build-preload --watch",
    "dev:main": "cross-env NODE_ENV=development node script/build-main --watch",
    "build:render": "vite build --mode=development",
    "build:preload": "cross-env NODE_ENV=production node script/build-preload",
    "build:main": "cross-env NODE_ENV=production node script/build-main",
    "build:electron": "cross-env NODE_OPTIONS=--max_old_space_size=5120 node script/build-electron",
    "visualizer": "cross-env VISUALIZER=true vite build --mode=production",
    "lint:eslint": "eslint src/**/*.{ts,vue} --fix",
    "lint:stylelint": "stylelint src/**/*.{vue,scss,less} --fix",
    "prepare": "husky install",
    "preinstall": "node script/install/preinstall",
    "postinstall": "node script/install/postinstall",
    "electron:dep": "electron-builder install-app-deps",
    "predeploy": "vite build --mode=web --base=qjkhdg",
    "deploy": "node script/deploy.js -s -n qjkhdg -d",
    "deploy:electron": "cross-env NODE_OPTIONS=--max_old_space_size=5120 node script/deploy-electron.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@element-plus/icons": "^0.0.11",
    "@sentry/electron": "^4.17.0",
    "@sentry/vue": "^7.93.0",
    "@tinymce/tinymce-vue": "^4.0.4",
    "@vueuse/core": "^6.7.3",
    "axios": "^0.21.4",
    "dayjs": "^1.11.7",
    "echarts": "^5.3.0",
    "echarts-wordcloud": "^2.0.0",
    "electron-context-menu": "^3.1.2",
    "electron-log": "^4.4.1",
    "electron-store": "^8.0.1",
    "electron-updater": "^5.3.0",
    "element-plus": "2.1.4",
    "hls.js": "~1.1.5",
    "html2canvas-objectfit-fix": "^1.2.0",
    "nprogress": "^0.2.0",
    "pinyin-match": "^1.2.4",
    "qrcode.vue": "^3.3.3",
    "qs": "^6.10.1",
    "shortid": "^2.2.16",
    "swiper": "^9.2.0",
    "vue": "^3.4.8",
    "vue-router": "^4.2.5",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/minimist": "^1.2.5",
    "@types/node": "^18.16.0",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@types/shortid": "^0.0.32",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vitejs/plugin-vue": "^5.0.2",
    "@vue/compiler-sfc": "^3.4.7",
    "adm-zip": "^0.5.10",
    "archiver": "^6.0.1",
    "asar": "^3.2.0",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^12.0.0",
    "electron-builder": "^24.9.1",
    "electron-notarize": "^1.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.19.2",
    "extract-zip": "^2.0.1",
    "fs-extra": "^8.1.0",
    "husky": "^8.0.3",
    "inquirer": "^8.2.0",
    "lint-staged": "^15.2.0",
    "minimist": "^1.2.8",
    "node-ssh": "^13.1.0",
    "ora": "^5.4.0",
    "prettier": "^3.1.1",
